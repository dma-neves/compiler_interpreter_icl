def number=5 indice=new 2 res=new 1 in

    while !indice <= number
    do
        res := !res * !indice;
        indice := !indice + 1
    end;

    print !res

end

/* -------------------- single line ---------------- */

def number=5 indice=new 2 res=new 1 in while !indice <= number do res := !res * !indice; indice := !indice + 1 end; print !res end